{
  "name": "dilip-singh-portfolio",
  "version": "1.0.0",
  "description": "Dilip Singh's Senior Developer Portfolio - Built with Next.js and TypeScript",
  "private": true,

  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "export": "next build && next export",
    "deploy": "npm run build && touch out/.nojekyll && git add out/ && git commit -m 'Deploy to GitHub Pages' && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "@emailjs/browser": "^3.7.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "aos": "^2.3.4",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.541.0",
    "next": "^14.0.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^4.0.0",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^9.5.0",
    "react-toastify": "^9.1.3",
    "react-vanilla-tilt": "^1.0.0",
    "tailwind-merge": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",

    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
